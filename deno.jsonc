{
  "name": "@pomdtr/fsrouter",
  "version": "0.2.0",
  "description": "A simple filesystem router for Deno",
  "exports": {
    ".": "./mod.ts"
  },
  "tasks": {
    "start": "deno run -A --watch=example/pages example/mod.ts",
    "format": "deno fmt",
    "lint": "deno lint",
    "test": "deno test -A test"
  },
  "publish": {
    "include": [
      "README.md",
      "LICENSE",
      "mod.ts",
      "core/*.ts"
    ]
  },
  "fmt": {
    // Don't try and format this autogenerated file
    "exclude": [
      "CHANGELOG.md"
    ]
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.6",
    "@std/fmt": "jsr:@std/fmt@^1.0.2",
    "@std/fs": "jsr:@std/fs@^1.0.4",
    "@std/log": "jsr:@std/log@^0.224.8",
    "@std/path": "jsr:@std/path@^1.0.6",
    "@std/testing": "jsr:@std/testing@^1.0.3"
  }
}
